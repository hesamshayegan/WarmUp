{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/quizProgress.json",
    "type": "object",
    "properties": {
      "user_id": {
        "type": "integer"
      },
      "cat_id": {
        "type": "integer"
      },
      "questions_per_category": {
        "type": "integer"
      },
      "correct_answers": {
        "type": "integer",
        "minimum": 0,
        "maximum": 30
      },
      "current_complexity": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "correct_answers"
    ]
  }
  